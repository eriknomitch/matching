#!/bin/zsh

source .env

paperspace jobs create \
  --container "Paperspace + Fast.AI (JupyterLab BETA)" \
  --machineType "GPU+" \
  --command "./run.sh" \
  --workspace "https://github.com/eriknomitch/matching.git" \
  --workspaceUsername $GITHUB_USERNAME \
  --workspacePassword $GITHUB_PERSONAL_ACCESS_TOKEN \
  --projectId "pr4cdzin8"
